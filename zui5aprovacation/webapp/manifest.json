{
  "_version": "1.17.0",
  "sap.app": {
    "id": "polar.zui5aprovacation",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.10.0",
      "toolsId": "9da1446c-74e7-4665-8b1a-9118692a6273"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZHR_CO_FIORI_ESS_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.71.50",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "polar.zui5aprovacation.i18n.i18n"
        }
      },
      "listvacationrequest": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/ListVacationRequest.json",
        "settings": {
            "defaultBindingMode": "TwoWay"
        }
    },
    "detailvacationrequest": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/DetailVacationRequest.json",
        "settings": {
            "defaultBindingMode": "TwoWay"
        }
    },
    "validateserv": {
      "type": "sap.ui.model.json.JSONModel",
      "uri": "model/ValidateServ.json",
      "settings": {
        "defaultBindingMode": "TwoWay"
      }
    },
    "listquota": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/ListQuota.json",
        "settings": {
            "defaultBindingMode": "TwoWay"
        }
    }

    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "polar.zui5aprovacation.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "pattern": "",
          "name": "vacationrequestaprov",
          "target": [
              "vacationrequestaprov"
          ]
      },
      {
        "pattern": "NotFound",
        "name": "notfound",
        "target": [
            "notfound"
        ]
    }
      ],
      "targets": {
        "vacationrequestaprov": {
          "viewName": "VacationRequestAprov",
          "viewId": "VacationRequestAprov",
          "viewLevel": 3,
          "title": "{i18n>EmploymentLetterViewTitle}"
      },
      "notfound": {
          "viewName": "NotFound",
          "viewId": "notfound",
          "viewLevel": 1,
          "title": "{i18n>LogonViewTitle}"
      }
      }
    },
    "rootView": {
      "viewName": "polar.zui5aprovacation.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
